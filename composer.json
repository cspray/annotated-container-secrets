{
  "name": "cspray/annotated-container-secrets",
  "description": "An Annotated Container ParameterStore to allow injecting configuration values from a variety of sources.",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "adbario/php-dot-notation": "^3.3",
    "cspray/annotated-container": "^2.1"
  },
  "require-dev": {
    "cspray/phinal": "^2.0",
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.12"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\AnnotatedContainer\\Secrets\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\AnnotatedContainer\\Secrets\\Test\\": "tests"
    }
  }
}